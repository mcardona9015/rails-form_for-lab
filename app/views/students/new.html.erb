<h1>Student Form</h1>

<%if flash[:messages] %>
    <% flash[:messages].each do |message| %>
        <p style="color:Red"><%=message%></p>
    <%end%>
<%end%>
<%= form_for @student do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.submit %>

<%end%>
